(doctype :html5)
[:html
 [:head
  [:meta {:http-equiv "Content-Type" :content "text/html" :charset "iso-8859-1"}]
  [:title "{{name}}"]
  (include-css "/stylesheets/{{name}}.css")
  (include-js (format "/javascript/{{name}}%s.js" (if (env/development-env?) "_dev" "")))]
 [:body
  (eval (:template-body joodo.views/*view-context*))
]]